@model List<ViewProject>

@{
    ViewBag.Title = "Мои проекты" + " — EduTeam";
    Layout = "_Layout";
}

<head>
    <link rel="stylesheet" href="~/css/site.css"/>
</head>
<body>
<div>
    <div>
        <h2>Мои проекты</h2><hr/>

        @foreach (var project in Model)
        {
            @(await Html.PartialAsync("~/Views/Projects/ProjectSummary.cshtml", project))
        }
        <div style="flex-basis: 100%;  height: 0"></div>
        <a class="btn btn-primary btn-sm ml-lg-5" href="@Url.Action("CreateProject", "Projects")">Создать новый проект</a>
                        
        
    </div>
</div>
</body>